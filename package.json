{
  "name": "easy-paper",
  "version": "1.4.0",
  "description": "The easist way to write academic papers",
  "main": "./out/main/index.js",
  "author": "LeafYeeXYZ",
  "homepage": "https://github.com/LeafYeeXYZ/EasyPaper",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeafYeeXYZ/EasyPaper"
  },
  "license": "GPL-3.0-only",
  "keywords": [
    "electron",
    "vite",
    "react",
    "typescript",
    "markdown",
    "latex",
    "katex",
    "academic",
    "paper",
    "editor"
  ],
  "scripts": {
    "format": "prettier --write .",
    "version": "commit-and-tag-version",
    "push": "git push --follow-tags origin main",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:both": "electron-vite build && electron-builder --mac --arm64 && electron-builder --win --x64"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "antd": "^5.21.1",
    "katex": "^0.16.11",
    "marked": "^14.1.2",
    "marked-katex-extension": "^5.1.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "commit-and-tag-version": "^12.4.4",
    "electron": "^31.6.0",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}
